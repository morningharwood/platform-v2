{
  "name": "morningharwood",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "apps:affected": "node ./node_modules/@nrwl/schematics/src/affected/run-affected.js apps",
    "build:affected": "node ./node_modules/@nrwl/schematics/src/affected/run-affected.js build",
    "e2e:affected": "node ./node_modules/@nrwl/schematics/src/affected/run-affected.js e2e",
    "format": "node ./node_modules/prettier/bin/prettier.js --single-quote --print-width 120 --write \"{apps,libs}/**/*.ts\"",
    "nx-migrate": "node ./node_modules/@nrwl/schematics/src/migrator/migrator.js",
    "deploy": "node_modules/.bin/ng build --prod && node_modules/.bin/firebase deploy",
    "server:build": "node_modules/.bin/ng build -prod --app=portfolio-server --verbose --sm",
    "prerender:bundle:dev": "node_modules/.bin/ts-node -r tsconfig-paths/register libs/prerender/src/bundle.ts",
    "prerender:bundle:prod": "node_modules/.bin/ts-node -r tsconfig-paths/register libs/prerender/src/prerender.ts",
    "ngsw-config": "node_modules/.bin/ngsw-config apps/portfolio/dist/apps/portfolio apps/portfolio/src/ngsw-config.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js apps/portfolio/dist/apps/portfolio",
    "build-prod-ngsw": "npm run prerender:bundle:dev && npm run ngsw-config && npm run ngsw-copy",
    "serve-prod-ngsw": "npm run build-prod-ngsw && http-server dist/apps/portfolio -p 8080",
    "serve:dist:local": "npx http-server /dist/apps/portfolio"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.2",
    "@angular/cdk": "^5.0.2",
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/forms": "^5.1.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "@angular/platform-server": "^5.1.2",
    "@angular/router": "^5.1.2",
    "@angular/service-worker": "^5.1.2",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/entity": "^4.1.1",
    "@ngrx/router-store": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "4.0.0",
    "@nrwl/nx": "0.6.0",
    "core-js": "2.5.3",
    "gl-matrix-ts": "^2.3.3",
    "global": "^4.3.2",
    "lodash": "4.17.4",
    "rxjs": "^5.5.2",
    "youtube-player": "^5.4.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "nrwl/fix-cli-build#release16",
    "@angular/compiler-cli": "`^5.1.2`",
    "@angular/language-service": "^5.1.2",
    "@nestjs/common": "^4.5.2",
    "@nestjs/core": "4.5.2",
    "@nestjs/microservices": "4.5.2",
    "@nestjs/testing": "4.5.2",
    "@nestjs/websockets": "4.5.2",
    "@nrwl/schematics": "0.6.0",
    "@types/fs-extra": "5.0.0",
    "@types/jasmine": "^2.8.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "4.14.91",
    "@types/node": "~8.5.2",
    "@types/yargs": "10.0.0",
    "@types/youtube": "^0.0.31",
    "body-parser": "^1.18.2",
    "codelyzer": "^4.0.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "firebase-tools": "3.16.0",
    "fs-extra": "5.0.0",
    "http-server": "^0.10.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "^1.9.2",
    "protractor": "~5.2.2",
    "reflect-metadata": "^0.1.10",
    "stylelint": "8.4.0",
    "stylelint-config-recommended": "2.0.1",
    "stylelint-config-sass-guidelines": "4.0.1",
    "stylelint-order": "0.8.0",
    "stylelint-scss": "2.2.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^2.6.0",
    "tslint": "~5.7.0",
    "typescript": "2.5.3",
    "yargs": "10.0.3"
  }
}
